<div class="row">
  <div class="col-md-6 col-md-offset-3">

    <%= form_for(@product) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :appname %>
      <%= f.text_field :appname %>
      
      <%= f.submit "Post", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>